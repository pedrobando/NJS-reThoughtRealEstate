{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/components/*": ["app/_components/*"],
        "@/utils/*": ["app/_utils/*"],
        "@/models/*": ["app/_models/*"],
        "@/libs/*": ["app/_libs/*"],
        "@/public/*": ["public/*"],
        "@/app/*": ["app/*"],
      }
    },
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "**/*.js",
      "**/*.jsx"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  