{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["app/_components/*"],
      "@/utils/*": ["app/_utils/*"],
      "@/models/*": ["app/_models/*"],
      "@/libs/*": ["app/_libs/*"],
      "@/public/*": ["public/*"],
      "@/app/*": ["app/*"],
      "@/*": ["./*"],
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next",
      },
    ],
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    ".next/types/**/*.ts",
  ],
  "exclude": ["node_modules"],
}
